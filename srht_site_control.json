{
    "notFound": "404.html",
    "fileConfigs": [
        {
            "glob": "*.html",
            "options": {
                "cacheControl": "cache-control: public, max-age=0, must-revalidate"
            }
        },
        {
            "glob": "page-data/app-data.json",
            "options": {
                "cacheControl": "cache-control: public, max-age=0, must-revalidate"
            }
        },
        {
            "glob": "page-data/**/page-data.json",
            "options": {
                "cacheControl": "cache-control: public, max-age=0, must-revalidate"
            }
        },
        {
            "glob": "static/**",
            "options": {
                "cacheControl": "cache-control: public, max-age=31536000, immutable"
            }
        },
        {
            "glob": "*.{js,css}",
            "options": {
                "cacheControl": "cache-control: public, max-age=31536000, immutable"
            }
        },
        {
            "glob": "sw.js",
            "options": {
                "cacheControl": "cache-control: public, max-age=0, must-revalidate"
            }
        }
    ]
}
